{
  "private": "true",
  "name": "fuseement",
  "workspaces": [
    "packages/*",
    "site"
  ],
  "version": "1.0.0",
  "repository": "git@github.com:fusee-code-lab/fuseement.git",
  "author": "fusee-code-lab",
  "scripts": {
    "start": "yarn workspace site start",
    "dev:client": "start-storybook -p 6006",
    "dev:client-force": "start-storybook -p 6006 --no-manager-cache",
    "dev:api": "yarn workspace @fuseement/server-api start",
    "dev:dash": "yarn workdpace @fuseement/dashboard dev",
    "build:client": "yarn workspace @fuseement/client-components bundle",
    "build:react": "yarn workspace @fuseement/react-components bundle",
    "build:vue": "yarn workspace @fuseement/vue-components bundle",
    "fix:prettier": "prettier \"packages/**/*.{ts,tsx,svelte,vue}\" --write",
    "check:prettier": "prettier \"packages/**/*.{ts,tsx,svelte,vue}\" --list-different"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.3",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/svelte": "^6.3.4",
    "@types/node": "^16.3.2",
    "@vue/compiler-sfc": "^3.1.5",
    "postcss": "^8.3.5",
    "prettier": "2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "rollup": "^2.53.1",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.35.2",
    "svelte": "^3.38.3",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.7.4",
    "typescript": "^4.3.5",
    "vercel": "^23.0.1"
  }
}
